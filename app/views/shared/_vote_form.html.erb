<div id="<%= review.id %>vote_form">
	<% if current_user.voted?(review) %>
		<% if current_user.upvoted?(review) %>
		  <%= render 'votes/upvoted', :review => review %>
		<% else %>
		  <%= render 'votes/downvoted', :review => review %>
		<% end %>
	<% else %>
		<%= render 'votes/vote', :review => review %>
	<% end %>
</div>